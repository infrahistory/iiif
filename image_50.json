                                {
                                    "type": "TextualBody",
                                    "language": "en",
                                    "format": "text/html",
                                    "value": "<p><span class=\"field-label\"><b>Title</b></span>: <span class=\"field-value\">Policeman on point duty</span></p>",
                                    "generator": "iaw/data/publications/annotation-template/01K6MV7H5TVMWM1NQVK6SMS4EB/field/1"
                                },
                                {
                                    "type": "TextualBody",
                                    "language": "en",
                                    "format": "text/html",
                                    "value": "<p class=\"field-label\"><b>Description</b></p><div class=\"field-value\">Although the City Council trialled several automatic traffic control devices through the early 1920s, there was a feeling that they were not as able to cope with Melbourne's extraordinary volume and variety of traffic.</div>",
                                    "generator": "iaw/data/publications/annotation-template/01K6MV7H5TVMWM1NQVK6SMS4EB/field/2"
                                }
                            ]
                        },
