                            "body": [
                                {
                                    "type": "TextualBody",
                                    "language": "en",
                                    "format": "text/html",
                                    "value": "<p><span class=\"field-label\"><b>Title</b></span>: <span class=\"field-value\">Princes Bridge Station</span></p>",
                                    "generator": "iaw/data/publications/annotation-template/01K6MV7H5TVMWM1NQVK6SMS4EB/field/1"
                                },
                                {
                                    "type": "TextualBody",
                                    "language": "en",
                                    "format": "text/html",
                                    "value": "<p class=\"field-label\"><b>Description</b></p><div class=\"field-value\">Flinders Street's less glamorous cousin, Princes Bridge was the terminus for north-eastern suburban lines. It was gradually incorporated into the larger station over the course of decades.</div>",
                                    "generator": "iaw/data/publications/annotation-template/01K6MV7H5TVMWM1NQVK6SMS4EB/field/2"
                                },
                                {
                                    "type": "TextualBody",
